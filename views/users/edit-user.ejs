<body>
    <!-- INCLUDE HEADER -->
    <%- include('../partials/header') %>

        <main>

            <form action="/user/edit/<%= locals.oldData ? oldData.id_users : null %>?_method=PUT" method="POST"
                id="form" enctype="multipart/form-data">
                <div class="form">
                    <h1>Actualizar Información</h1>
                    <div class="grupo">
                        <span class="barra">
                            <ul class="ul-errors">

                            </ul>
                        </span>
                    </div>
                    <div class="grupo">
                        <input type="text" name="first_name" id="first_name"
                            value="<%= locals.oldData ? oldData.first_name : null %>" />
                        <% if (locals.errors && errors.first_name) { %>
                            <span class="barra">
                                <%= errors.first_name.msg %>
                            </span>
                            <% } %>
                                <label for="nombre">Nombre</label>
                    </div>
                    <div class="grupo">
                        <input type="text" name="last_name" id="last_name"
                            value="<%= locals.oldData ? oldData.last_name : null %>" />
                        <% if (locals.errors && errors.last_name) { %>
                            <span class="barra">
                                <%= errors.last_name.msg %>
                            </span>
                            <% } %>
                                <label for="">Apellido</label>
                    </div>
                    <div class="grupo">
                        <input type="email" name="email" id="email"
                            value="<%= locals.oldData ? oldData.email : null %>" />
                        <% if (locals.errors && errors.email) { %>
                            <span class="barra">
                                <%= errors.email.msg %>
                            </span>
                            <% } %>
                                <label for="">Email</label>
                    </div>
                    <div class="grupo">
                        <input type="password" name="password" id="password" />
                        <% if (locals.errors && errors.password) { %>
                            <span class="barra">
                                <%= errors.password.msg %>
                            </span>
                            <% } %>
                                <label for="n">Contraseña</label>
                    </div>
                    <div class="grupo">
                        <input type="password" name="secondPassword" id="secondPassword" />
                        <% if (locals.errors && errors.secondPassword) { %>
                            <span class="barra">
                                <%= errors.secondPassword.msg %>
                            </span>
                            <% } %>
                                <label for="">Repetir Contraseña</label>
                    </div>
                    <div class="grupo">
                        <input type="text" name="province" id="province"
                            value="<%= locals.oldData ? oldData.province : null %>" />
                        <% if (locals.errors && errors.province) { %>
                            <span class="barra">
                                <%= errors.province.msg %>
                            </span>
                            <% } %>
                                <label for="">Provincia</label>
                    </div>
                    <div class="grupo">
                        <input type="text" name="location" id="location"
                            value="<%= locals.oldData ? oldData.location : null %>" />
                        <% if (locals.errors && errors.location) { %>
                            <span class="barra">
                                <%= errors.location.msg %>
                            </span>
                            <% } %>
                                <label for="">Localidad</label>
                    </div>

                    <div class="grupo">
                        <div class="image-upload-wrap">
                            <input class="file-upload-input" type='file' name="avatar" id="avatar"
                                onchange="readURL(this);" accept="image/*"
                                value="<%= locals.oldData ? oldData.avatar : null %>" />
                            <div class="drag-text">
                                <h3>Arrastra o selecciona una imagen</h3>
                            </div>
                        </div>
                        <% if (locals.errors && errors.avatar) { %>
                            <span class="barra">
                                <%= errors.avatar.msg %>
                            </span>
                            <% } %>
                    </div>
                    <div class="grupo">
                        <input type="text" name="street" id="street"
                            value="<%= locals.oldData ? oldData.street : null %>" />
                        <% if (locals.errors && errors.street) { %>
                            <span class="barra">
                                <%= errors.street.msg %>
                            </span>
                            <% } %>
                                <label for="">Calle - altura</label>
                    </div>
                    <input type="hidden" name="profile" id="profile"
                        value="<%= locals.oldData ? oldData.id_profile : null %>" />

                    <button class="register-button" type="submit">Actualizar</button>
                </div>
            </form>
        </main>
        <script src="/js/validationUser.js"></script>
        <!-- INCLUDE FOOTER -->
        <%- include('../partials/footer') %>
</body>

</html>