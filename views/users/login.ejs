<body>
    <!-- INCLUDE HEADER -->
    <%- include('../partials/header') %>

        <main>
            <form class="form" method="POST" id="form" action="/user/login">
                <div class="grupo">
                    <span class="barra">
                        <ul class="ul-errors">

                        </ul>
                    </span>
                </div>
                <div class="form">
                    <h1>Inicía Sesión</h1>

                    <div class="grupo">
                        <input type="email" class="usuario" name="email" id="email">
                        <% if (locals.errors && errors.email) { %>
                            <span class="barra">
                                <%= errors.email.msg %>
                            </span>
                            <% } %>
                                <label>Usuario</label>
                    </div>

                    <div class="grupo">
                        <input type="password" class="usuario" name="password" id="password">
                        <% if (locals.errors && errors.password) { %>
                            <span class="barra">
                                <%= errors.password.msg %>
                            </span>
                            <% } %>
                                <label>Password</label>
                    </div>
                    <div class="grupo group-remember">
                        <input type="checkbox" class="usuario" name="remember">
                        <label class="remember">Recordar usuario</label>
                    </div>

                    <button class="button-login">Iniciar sesion<a href="user/login"></a></button>
                </div>
                <p>No tienes una cuenta? <a href="/user/register">Registrate aqui</a></p>
            </form>
        </main>
        <script src="/js/validationLogin.js"></script>

        <!-- INCLUDE FOOTER -->
        <%- include('../partials/footer') %>
</body>

